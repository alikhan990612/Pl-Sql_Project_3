		<parameter name="vvody" class="java.lang.String"/>
		<queryString>
			<![CDATA[select
'Взаиморасчеты с абонентами' shapka,
ltrim(
    case when nvl($P{raion},'emp')!='emp' then
    ', Район: '||spr#function.raion($P{raion})
    end ||
    case when nvl($P{grup},-1)!=-1 then
    ', Группа: '||spr#function.grup($P{grup})
    end ||
    case when nvl($P{uch},'emp')!='emp' then
        ', Участок: '||spr#function.uchastok($P{uch})
    end ||
    case when nvl($P{kotel},'emp')!='emp' then
        ', Котельная: '||spr#function.kotel($P{kotel})
    end ||
    case when nvl($P{vvody},-1)!=-1 then
    ', Ввод: '||spr#function.vvody($P{vvody})
    end ||
    case when nvl($P{sector},-1)!=-1 then
    ', Сектор: '||spr#function.sector($P{sector})
    end
,', ') metka
from dual]]>
		</queryString>
		<field name="SHAPKA" class="java.lang.String"/>
		<field name="METKA" class="java.lang.String"/>
	</subDataset>
